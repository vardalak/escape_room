{
  "expo": {
    "name": "Escape Room",
    "slug": "escape_room",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.escaperoom"
    },
    "android": {
      "package": "com.yourcompany.escaperoom"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
